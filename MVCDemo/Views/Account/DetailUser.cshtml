@model MVCDemo.Models.UserInfo
@{
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}
@section TitleSection{
    DetailUser
}
@section HeaderSection{
    <link href="~/Content/Site.css" rel="stylesheet" />
}
@section ContentBody{
    <div class="body-content container">
        <dl>
            <dt>编号：</dt>
            <dd>@Html.DisplayFor(model=>model.ID)</dd>

            <dt><br />姓名：</dt>
            <dd>@Html.DisplayFor(model => model.UserName)</dd>

            <dt><br />密码：</dt>
            <dd>@Html.DisplayFor(model => model.UserPwd)</dd>

            <dt><br />邮箱：</dt>
            <dd>@Html.DisplayFor(model => model.UserEmail)</dd>

            <dt><br />地址：</dt>
            <dd>@Html.DisplayFor(model => model.Address)</dd>
        </dl>
        <div>
            @Html.ActionLink("返回首页", "IndexUser")
        </div>
    </div>
}


